{
    "symlinks": {
        "command": "<%= symfony_console %> assets:install <%= project_source %>/web --relative --symlink"
    },
    "assetic": {
        "command": "<%= symfony_console %> assetic:dump --env=prod --no-debug"
    },
    "trans":{
        "command": "<%= symfony_console %> translation:update --clean --force de {REPLACE}"
    }
}
